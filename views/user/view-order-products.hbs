<section>
  {{#each products}}
    <div class="container">
      <div class="content" style="margin-top: 5%;">
        <img
          src="/product-images/{{this.product._id}}.jpg"
          alt=""
          style="width: 300px; height: auto; float: left;"
        />
        <div class="details" style="padding-bottom: 10%;">
          <h4 style="padding-top: 5%;">{{this.product.Product}}</h4>
          <p>{{this.product.Category}}</p>
          <p>Quantity {{this.quantity}}</p>
          <h4>{{this.product.Price}}</h4>
        </div>
      </div>
    </div>
    <br>
  {{/each}}
</section>